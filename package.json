    "sass-resources-loader": "^1.1.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.3.1",
    "stylelint-config-standard": "^14.0.0",
    "stylelint-scss": "^1.3.4",
    "svg-sprite-loader": "0.0.31",
    "tap-notify": "^1.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "tape-catch": "^1.0.6",
    "webpack": "^1.13.0",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.1"
  }
}
