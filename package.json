{
  "name": "react-challenge-colorizr",
  "version": "1.0.0",
  "description": "Boilerplate for second React Challenge",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint:eslint && npm run lint:stylelint && npm run test:only",
    "test:cache": "npm run lint:eslint-cache && npm run lint:stylelint && npm run test:only",
    "test:only": "cross-env NODE_ENV=development tape --require babel-register --require ignore-styles src/**/*.test.{js,jsx} | tap-notify | tap-spec",
    "start": "npm run presets && cross-env NODE_ENV=development webpack-dashboard -- node scripts/start",
    "start:storybook": "start-storybook -p 3003 -c storybook",
    "build:storybook": "build-storybook -c storybook -o public/storybook",
    "build": "cross-env NODE_ENV=production webpack --progress --colors && npm run build:storybook",
    "clean": "rimraf public",
    "lint": "npm run lint:eslint-fix && npm run lint:stylelint",
    "lint:eslint": "eslint src --ext .js --ext .jsx",
    "lint:eslint-fix": "eslint src --ext .js --ext .jsx --fix",
    "lint:eslint-cache": "eslint src --ext .js --ext .jsx --cache",
    "lint:stylelint": "stylelint **/*.scss",
    "gh:build": "npm run clean && npm run presets && npm run build:storybook && cross-env NODE_ENV=production BUILD=pages webpack --progress --colors",
    "gh:deploy": "npm run gh:build && gh-pages -d public",
    "deploy": "npm run gh:deploy",
    "presets": "node scripts/generate"
  },
  "pre-push": [
    "test:cache"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtivital/react-challenge-colorizr.git"
  },
  "keywords": [
    "react",
    "react-challenge",
    "react-router",
    "redux",
    "react-redux"
  ],
  "author": "Vitaly Rtishchev <rtivital@gmail.com> (http://github.com/rtivital)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtivital/react-challenge-colorizr/issues"
  },
  "homepage": "https://github.com/rtivital/react-challenge-colorizr#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "breakpoint-sass": "^2.7.0",
    "classnames": "^2.2.5",
    "lodash": "^4.16.6",
    "node-uuid": "^1.4.7",
    "normalize.css": "^5.0.0",
    "rbem": "^1.0.1",
    "react": "^15.3.2",
    "react-color-picker": "^4.0.2",
    "react-copy-to-clipboard": "^4.2.3",
    "react-dom": "^15.3.2",
    "react-fastclick": "^2.1.2",
    "react-onclickoutside": "^5.7.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-beta.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.5.2",
    "redux-actions": "^0.13.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-module-resolver": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-babel-module": "^2.0.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
